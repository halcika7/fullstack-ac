{
  "name": "express-ts",
  "version": "0.0.1",
  "description": "Ant-Colony API",
  "author": "",
  "license": "ISC",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "start": "node dist/server.js",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src --watch .env.development -L -e ts --exec yarn dev:start",
    "test": "jest --runInBand --no-cache",
    "test:coverage": "yarn test --coverage --detectOpenHandles",
    "lint": "eslint ./src ./__tests__ --ext .ts --fix",
    "tsc": "tsc --project tsconfig.prod.json && tsc-alias -p tsconfig.json",
    "build": "npm-run-all clean lint tsc"
  },
  "dependencies": {
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
